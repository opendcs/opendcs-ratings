<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2020.1/project-config.xsd">
  <name>Build and Test</name>
  <description />
  <settings>
    <options>
      <option name="artifactRules" value="build/libs/*.jar =&gt; /&#xA;build/sonar/report-task.txt =&gt;" />
      <option name="executionTimeoutMin" value="15" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_1" name="build Project" type="gradle-runner">
        <parameters>
          <param name="teamcity.step.mode" value="default" />
          <param name="ui.gradleRunner.gradle.tasks.names" value=":build" />
        </parameters>
      </runner>
      <runner id="RUNNER_2" name="test Project" type="gradle-runner">
        <parameters>
          <param name="teamcity.step.mode" value="default" />
          <param name="ui.gradleRunner.gradle.tasks.names" value=":test" />
        </parameters>
      </runner>
      <runner id="RUNNER_3" name="" type="gradle-runner">
        <parameters>
          <param name="teamcity.step.mode" value="default" />
          <param name="ui.gradleRunner.additional.gradle.cmd.params" value="-Dsonar.login=%system.SONAR_TOKEN% -Dsonar.host.url=https://sonarqube.hecdev.net" />
          <param name="ui.gradleRunner.gradle.tasks.names" value=":sonarqube" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="SettingsRootId" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="TRIGGER_1" type="vcsTrigger">
        <parameters />
      </build-trigger>
    </build-triggers>
    <build-extensions>
      <extension id="BUILD_EXT_1" type="commit-status-publisher">
        <parameters>
          <param name="publisherId" value="atlassianStashPublisher" />
          <param name="secure:stashPassword" value="credentialsJSON:stashPassword" />
          <param name="stashBaseUrl" value="https://bitbucket.hecdev.net" />
          <param name="stashUsername" value="builduser" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_2" type="halfbaked-sonarqube-report-plugin">
        <parameters />
      </extension>
      <extension id="BUILD_EXT_3" type="BuildFailureOnMetric">
        <parameters>
          <param name="metricKey" value="VisibleArtifactsSize" />
          <param name="metricThreshold" value="1MB" />
          <param name="metricUnits" value="metricUnitsDefault" />
          <param name="moreOrLess" value="more" />
          <param name="stopBuildOnFailure" value="true" />
          <param name="withBuildAnchor" value="false" />
        </parameters>
      </extension>
    </build-extensions>
    <cleanup />
  </settings>
</build-type>

